{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "paths": {
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@services/*": [
        "./src/service/*"
      ],
      "@/*": [
        "./src/*"
      ],
    },
    "types": ["node"]
  }
}